{ //this is the manifest file where all the configuration for the extension happens
  "name": "Sample Extension",
  "version": "1.0",
  "description": "Sample Extenstion",
  "background_page":"background.html",
  "content_scripts": [
    {
      "matches": ["http://*.com/*"],
      "js": ["content_alter.js"]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
	"popup": "popup.html"
  }
}